idfromteman = []
# DUMP PUBLIK #
def publik():
    try:
        toket = open('login.txt', 'r').read()
    except IOError:
        print ' [!] Invalid'
        os.system('rm -rf login.txt')
        time.sleep(0.01)
        masuk()

    try:
        print ''
        print H+" [!] Ketik "+HU+"me"+H+" jika ingin dump daftar teman sendiri"
        idt = raw_input(' [?] Target id : ')
        try:
            jok = requests.get('https://graph.facebook.com/' + idt + '?access_token=' + toket)
            op = json.loads(jok.text)
            qq = (op['first_name'] + '.json').replace(' ', '_')
            print ' [*] Name : ' + op['name']
        except KeyError:
            exit(S+' [!] Id not found ').format('R')
            raw_input('\n [!] Back').format(N)
            menu()

        r = requests.get('https://graph.facebook.com/' + idt + '?fields=friends.limit(5000)&access_token=' + toket)
        z = json.loads(r.text)
        #print ' [!] Please Wait ...'
        print ''
        bz = open(qq, 'w')
        for a in z['friends']['data']:
            idfromteman.append(a['id'])
            bz.write(a['id'] + '<=>' + a['name'] + '\n')

        bz.close()
        print H+ ' [+] Succes dump id dari %s' % op['name']
        print '\r [+] Total id : %s' % len(idfromteman)
        print ' [+] File dump tersimpan : ' + qq
        print ''
        os.sys.exit()
    except Exception as e:
        exit('\n [!] Failed dump id')
    except requests.exceptions.ConnectionError:
        print ' [!] No Connection!'
        exit()

