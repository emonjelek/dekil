def pilih_masuk():
    romi = raw_input(H+' [?] : ')
    if romi == '':
        print S+ ' [!] Isi yg benar  '
        time.sleep(1.0)
        pilih_masuk()
    elif romi == '1' or romi == '01':
        token()
    elif romi == '2' or romi == '02':
        kuki()
    elif romi == '33' or romi == '033':
        tik()
        os.system('xdg-open https://youtu.be/IG5QfdxRkeY')
        os.sys.exit()
    elif romi == '44' or romi == '044':
        tik()
        os.system('xdg-open https://youtu.be/b9crrvr6d2s')
        os.sys.exit()
    elif romi == '000' or romi == '00':
    	print ''
    	print '\x1b[1;91m\xe2\x80\xa2 exit \x1b[0;97m\n'
        exit()
    else:
        print S+ ' [!] Isi yg benar  '
        time.sleep(1.0)
        pilih_masuk()

